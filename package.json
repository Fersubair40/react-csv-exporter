{
	"name": "@temilayodev/react-csv-exporter",
	"version": "1.0.0",
	"main": "dist/umd/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/esm/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/esm/index.js",
			"require": "./dist/umd/index.js",
			"types": "./dist/esm/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Fersubair40/react-csv-exporter.git"
	},
	"homepage": "",
	"scripts": {
		"build": "yarn clean && yarn build:esm && yarn build:umd && yarn copy-files",
		"build:esm": "tsc -p tsconfig.esm.json",
		"build:umd": "tsc -p tsconfig.umd.json",
		"clean": "rm -rf dist",
		"copy-files": "cp README.md dist/ && cp package.json dist/",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage"
	},
	"peerDependencies": {
		"react": "^18.0.0"
	},
	"devDependencies": {
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@types/jest": "^29.5.0",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"ts-jest": "^29.1.0",
		"typescript": "^5.0.0"
	},
	"keywords": [
		"csv",
		"export",
		"react",
		"typescript",
		"download"
	],
	"author": "Temilayo Subair <temisubair@icloud.com>",
	"license": "MIT"
}
